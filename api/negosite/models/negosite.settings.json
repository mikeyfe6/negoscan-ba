{
  "kind": "collectionType",
  "collectionName": "negosites",
  "info": {
    "name": "negosite",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "profiel": {
      "type": "string",
      "required": false,
      "unique": true,
      "maxLength": 15
    },
    "biografie": {
      "type": "richtext"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "background": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "bgfree": {
      "type": "enumeration",
      "enum": [
        "zwart",
        "grijs",
        "geel",
        "roze",
        "bruin"
      ],
      "default": "zwart"
    },
    "facebooklink": {
      "type": "string",
      "default": "#"
    },
    "instagramlink": {
      "type": "string",
      "default": "#"
    },
    "twitterlink": {
      "type": "string",
      "default": "#"
    },
    "website": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "gebruiker"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "private": false
    }
  }
}
